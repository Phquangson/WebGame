
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    @{
        Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
    }
</head>
<body>
    <!-- Banner-section start -->
    <section class="banner-section inner-banner position-relative store">
        <div class="shape-area">
            <img src="assets/images/abs-items/bubble.png" class="shape-1" alt="icon">
            <img src="assets/images/abs-items/ellipse-7.png" class="shape-2" alt="icon">
        </div>
        <div class="container position-relative">
            <div class="banner-content row justify-content-between">
                <div class="col-lg-6 col-md-10">
                    <div class="main-content">
                        <h2 class="visible-slowly-bottom display-four mb-4">
                            <span>
                                Get in touch with our friendly
                                team
                            </span>
                        </h2>
                        <p class="fs-four">
                            Our support goes all the way. We love hearing from customers and
                            visitors and are always happy to help.
                        </p>
                    </div>
                </div>
                <div class="col-lg-6 col-md-10 d-grid align-items-end justify-content-end">
                    <div class="breadcrumb-area position-absolute end-0 bottom-0">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb m-0 py-3 px-8 fs-six d-flex align-items-end justify-content-end">
                                <li class="breadcrumb-item p-0 m-0"><a href="@Url.Action("PageHome","Home")">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Contact Us</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Banner-section end -->
    <!-- Contact Us start -->
    <section class="contact-us bg-transparen pt-120 pb-120">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-6">
                    <div class="section-header text-center">
                        <h4 class="sub-title">Have <span>Questions?</span></h4>
                        <span class="fs-two heading mb-6">
                            Can’t find what are <span>you Looking for</span> ? Get
                            in Touch
                        </span>
                        <p>
                            Please fill out the form and let us know about your concerns.We will try our best to
                            provide optimized solutions.
                        </p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-lg-7">
                    <form action="#" class="p-4 p-sm-8">
                        <h3>Write A Message</h3>
                        <div class="form-inside mt-6 p-4">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="single-input text-start">
                                        <label for="name">Name</label>
                                        <input type="text" id="name" placeholder="Enter Your Name" autocomplete="off">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="single-input text-start">
                                        <label for="email">Email</label>
                                        <input type="text" id="email" placeholder="Enter your email" autocomplete="off">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="single-input text-start">
                                        <label for="subject">Subject</label>
                                        <input type="text" id="subject" placeholder="Enter Subject" autocomplete="off">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="single-input text-start">
                                        <label for="message">Leave us a message</label>
                                        <textarea cols="4" rows="4" id="message"
                                                  placeholder="Please type your Message here..."></textarea>
                                    </div>
                                </div>
                                <div class="col-sm-12 mt-4">
                                    <div class="btn-area">
                                        <button class="box-style btn-box">

                                            Send Massage
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="col-lg-5 mt-7 mt-lg-0">
                    <div class="py-8 py-sm-15 px-3 px-sm-6 get-in-touch">
                        <h3>Get in Touch</h3>
                        <div class="single-content d-grid mt-5 mt-sm-10">
                            <div class="single-content d-grid py-5 py-sm-7 gap-3 gap-sm-5">
                                <div class="check d-center justify-content-start gap-3">
                                    <i class="fa-solid fa-phone"></i>
                                    <h4>Have any Questions</h4>
                                </div>
                                <p class="d-flex gap-5">
                                    <span>(302) 555-0107</span>
                                    <span>(704) 555-0127</span>
                                </p>
                            </div>
                            <div class="single-content d-grid py-5 py-sm-8 gap-2">
                                <div class="check d-center justify-content-start gap-3">
                                    <i class="fa-solid fa-location-dot"></i>
                                    <h4>Contact Address</h4>
                                </div>
                                <p class="d-flex">
                                    <span>(302) 555-0107</span>
                                </p>
                            </div>
                            <div class="single-content d-grid py-5 py-sm-8 gap-2">
                                <div class="check d-center justify-content-start gap-3">
                                    <i class="fa-solid fa-clock"></i>
                                    <h4>Opening Hours</h4>
                                </div>
                                <p class="d-flex">
                                    <span>(302) 555-0107</span>
                                </p>
                            </div>
                        </div>
                        <ul class="d-flex justify-content-center gap-4 social-area py-4 py-sm-8 mt-5 mt-sm-10">
                            <li>
                                <a href="https://www.facebook.com" aria-label="Facebook" class="d-center">
                                    <i class="fab fa-facebook-f"></i>
                                </a>
                            </li>
                            <li>
                                <a href="https://www.instagram.com" aria-label="Instagram" class="d-center">
                                    <i class="fab fa-instagram"></i>
                                </a>
                            </li>
                            <li>
                                <a href="https://www.linkedin.com" aria-label="Linkedin" class="d-center">
                                    <i class="fab fa-linkedin-in"></i>
                                </a>
                            </li>
                            <li>
                                <a href="https://twitter.com" aria-label="Twitter" class="d-center">
                                    <i class="fab fa-twitter"></i>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
